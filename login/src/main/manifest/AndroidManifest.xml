<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.idisfkj.awesome.login">

    <application
            android:name=".LoginApp"
            android:allowBackup="true"
            android:icon="@mipmap/login_ic_launcher"
            android:label="@string/login_app_name"
            android:roundIcon="@mipmap/login_ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/LoginAppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity" />
    </application>

</manifest>